loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/MoonUI-v2/main/MoonLib.lua"))()local function ESP()for _,v in pairs(game.Players:GetPlayers())do if v~=game.Players.LocalPlayer and v.Character then local h=Instance.new("Highlight")h.Parent=v.Character h.FillColor=_G.PColor or Color3.fromRGB(255,0,0)h.OutlineColor=Color3.new(0,0,0)h.Adornee=v.Character end end end local function ScrapESP()for _,v in pairs(workspace:GetDescendants())do if v.Name:lower():find("scrap")and v:IsA("BasePart")then local h=Instance.new("Highlight")h.Parent=v h.FillColor=_G.SColor or Color3.fromRGB(255,215,0)h.OutlineColor=Color3.new(0,0,0)h.Adornee=v end end end local W=Library:CreateWindow("LunkerHub")local E=W:CreateTab("ESP")local P=E:CreateSection("Player ESP")P:CreateToggle("Enable",function(s)_G.PESP=s if s then ESP()else for _,v in pairs(game.Players:GetPlayers())do if v.Character and v.Character:FindFirstChild("Highlight")then v.Character.Highlight:Destroy()end end end end)P:CreateColorPicker("Color",function(c)_G.PColor=c if _G.PESP then ESP()end end)local S=E:CreateSection("Scrap ESP")S:CreateToggle("Enable",function(s)_G.SESP=s if s then ScrapESP()else for _,v in pairs(workspace:GetDescendants())do if v:IsA("BasePart")and v:FindFirstChild("Highlight")then v.Highlight:Destroy()end end end end)S:CreateColorPicker("Color",function(c)_G.SColor=c if _G.SESP then ScrapESP()end end)Library:Init()
